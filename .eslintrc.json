{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "off",
        "object-curly-spacing": ["error", "always", { "objectsInObjects": false }],
        "linebreak-style": "off",
        "semi": ["error", "never"],
        "max-len": "off",
        "react/prop-types": "warn"
    }
}
